@model List<Employee>

<h1>All Employees</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">title</th>
            <th scope="col">Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model){
            <tr>
                <td class="fw-bolder">@item.Id</td>
                <td class="fw-bolder">@item.Name</td>
                <td class="fw-bolder">@item.title</td>
                <td class="fw-bolder">@item.Age</td>
            </tr>
        }
       
       
    </tbody>
</table>
